/dts-v1/;
#include <st/f4/stm32f407Xg.dtsi>

/ {
	model = "STMicroelectronics STM32F4DISCOVERY board";
	compatible = "st,stm32f4discovery", "st,stm32f407";

	chosen {
		zephyr,console = &usart1;
		zephyr,shell-uart = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,ccm = &ccm0;
	};

	leds {
		compatible = "gpio-leds";
		orange_led_3: led_3 {
			gpios = <&gpioe 5 GPIO_INT_ACTIVE_LOW>;
			label = "User LD3";
		};
		green_led_4: led_4 {
			gpios = <&gpioe 6 GPIO_INT_ACTIVE_LOW>;
			label = "User LD4";
		};
		green_led_5: led_5 {
			gpios = <&gpiod 3 GPIO_INT_ACTIVE_HIGH>;
			label = "User LD5";
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		user_button: button {
			label = "Key";
			gpios = <&gpioe 3 GPIO_INT_ACTIVE_LOW>;
		};
	};

	aliases {
		led0 = &orange_led_3;
		led1 = &green_led_4;
		led2 = &green_led_5;
		sw0 = &user_button;
	};
};

&usart1 {
	current-speed = <115200>;
	pinctrl-0 = <&usart1_pins_a>;
	pinctrl-names = "default";
	status = "ok";
};

&usart3 {
	current-speed = <115200>;
	pinctrl-0 = <&usart3_pins_b>;
	pinctrl-names = "default";
	status = "ok";
};

&usart6 {
	current-speed = <115200>;
	pinctrl-0 = <&usart6_pins_b>;
	pinctrl-names = "default";
	status = "ok";
};

&timers2 {
	status = "disable";

	pwm {
		status = "disable";
	};
};

&rtc {
	status = "disable";
};

&usbotg_fs {
	status = "disable";
};
